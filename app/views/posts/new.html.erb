<div id="page_wrapper">
    <h2>Write some post</h2>
    <%= form_for :post, url: posts_path do |f|%>
        <p>
            <%= f.label :title %> <br>
            <%= f.text_field :title %>
        </p>
        <p>
            <%= f.label :body %> <br>
            <%= f.text_field :body %>
        </p>
        <p>
            <%= f.submit %>
        </p>
    <% end %>
</div>